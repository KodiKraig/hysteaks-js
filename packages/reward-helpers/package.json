{
  "name": "@hysteaks-js/reward-helpers",
  "version": "1.0.0",
  "description": "Helper functions for performing reward calculations off-chain.",
  "main": "./dist/cjs/index.js",
  "types": "./dist/cjs/index.d.ts",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KodiKraig/hysteaks-js.git"
  },
  "homepage": "https://www.hysteaks.com",
  "sideEffects": false,
  "license": "MIT",
  "private": false,
  "keywords": [
    "hysteaks",
    "hychain",
    "ethereum",
    "topia"
  ],
  "scripts": {
    "build": "tsc && tsc -p tsconfig.esm.json",
    "test": "mocha -r ts-node/register 'src/**/*.test.ts'"
  },
  "devDependencies": {
    "@types/chai": "^4.3.7",
    "@types/mocha": "^10.0.10",
    "chai": "^4.3.7",
    "mocha": "^11.0.1",
    "ts-node": "^10.9.2"
  }
}
